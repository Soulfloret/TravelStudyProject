<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<classPathEntry
		location="E:\path\to\local\repo\mysql\mysql-connector-java\5.1.6\mysql-connector-java-5.1.6.jar" />
	<context id="context1">
		<!-- 注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" /><!-- 是否取消注释 -->
			<property name="suppressDate" value="true" /> <!-- 是否生成注释代时间戳 -->
		</commentGenerator>
		<jdbcConnection
			connectionURL="jdbc:mysql://localhost:3306/student"
			driverClass="com.mysql.jdbc.Driver" password="123" userId="root" />
		<!-- domain生成位置 -->
		<javaModelGenerator
			targetPackage="com.accp.domain" targetProject="src/main/java" />
		<!-- mapper配置文件生成 -->
		<sqlMapGenerator targetPackage="mapper"
			targetProject="src/main/resources" />
		<!-- dao接口生成 -->
		<javaClientGenerator
			targetPackage="com.accp.dao" targetProject="src/main/java"
			type="XMLMAPPER" />
		<table schema="Student" tableName="student"></table>
	</context>
</generatorConfiguration>