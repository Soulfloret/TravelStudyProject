  <!-- 查询团体中的人 -->
  <select id="QueryTidUsers" resultMap="resultMap">
  		SELECT u.*,t.* FROM `team`  AS t 
		JOIN `teammember` AS tm ON t.`id`=tm.`teamId`
		JOIN `users` AS u ON  u.`id`=tm.`memberId`
		WHERE t.id=#{id}
  </select>
  
