  <select id="selectAllTeam" resultMap="MapYp">
  	 SELECT t.*,uname  FROM team AS t INNER JOIN users AS u ON t.`mainiUserId`=u.`id`
  	 <where>
  	 	<if test="tname!=null and tname!=''">
  	 	 	and t.name1 LIKE CONCAT('%',#{tname},'%')
  	 	</if>
  	 	<if test="uname!=null and uname!=''">
  	 	 	and u.`uName` LIKE CONCAT('%',#{uname},'%')
  	 	</if>
  	 </where>
  </select>
